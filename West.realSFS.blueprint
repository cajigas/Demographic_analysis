#granulifera blueprint file
#input setting
popid: West # id of the population (no white space)
nseq: 26 # number of sequences (2xn)
#ANGSD Arguments and parameters for all analysis are located in .arg file
L: 14425036 # Number of sites reported by angsd 
whether_folded: true # whethr the SFS is folded (true or false)
SFS: 51169.812061 31059.456815 21881.647562 17095.650730 14601.069466 12531.673947 11397.209027 10113.669805 9645.589315 9674.275205 9246.997564 9401.908320 7559.270171 # realSFS: singleton excluided, number of doubleton, etc. (separated by white space)
